{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "axios": "^1.8.4",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "cra-template": "1.2.0",
    "framer-motion": "^11.18.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.525.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.5.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^5.0.3"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "test:coverage": "craco test --coverage --run",
    "test:watch": "craco test --watch",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "test:component": "cypress open --component",
    "lint": "eslint src --ext .js,.jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "analyze": "set ANALYZE=true && npm run build",
    "analyze:legacy": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "analyze:server": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js --mode server",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@eslint/js": "9.23.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4.20",
    "axe-core": "^4.10.3",
    "cypress": "^14.5.2",
    "cypress-axe": "^1.6.0",
    "eslint": "9.23.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "15.15.0",
    "jest-environment-jsdom": "^30.0.4",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.17",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
